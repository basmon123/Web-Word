<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>55c91350-1234-4567-890a-bcdef1234567</Id>
  <Version>1.0.0.6</Version>
  <ProviderName>FDA Ingenieria</ProviderName>
  <DefaultLocale>es-ES</DefaultLocale>
  <DisplayName DefaultValue="EditorFDA"/>
  <Description DefaultValue="Suite de herramientas FDA."/>
  
  <IconUrl DefaultValue="https://basmon123.github.io/Web-Word/EditorFDA/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://basmon123.github.io/Web-Word/EditorFDA/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://github.com/basmon123/Web-Word"/>
  
  <AppDomains>
    <AppDomain>https://basmon123.github.io</AppDomain>
  </AppDomains>
  
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  
  <DefaultSettings>
    <SourceLocation DefaultValue="https://basmon123.github.io/Web-Word/EditorFDA/src/taskpane/taskpane.html"/>
  </DefaultSettings>
  
  <Permissions>ReadWriteDocument</Permissions>
  
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <CustomTab id="TabFDA">
              <Label resid="TabFDA.Label"/> 
              
              <Group id="GrupoFDA">
                <Label resid="GrupoFDA.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="TaskpaneButton">
                  <Label resid="TaskpaneButton.Label"/>
                  <Supertip>
                    <Title resid="TaskpaneButton.Label"/>
                    <Description resid="TaskpaneButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ShowTaskpane">
                    <TaskpaneId>ButtonId1</TaskpaneId>
                    <SourceLocation resid="Taskpane.Url"/>
                  </Action>
                </Control>
              </Group>

              <Group id="GrupoAcciones">
                <Label resid="GrupoAcciones.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>

                <Control xsi:type="Button" id="BtnLimpiar">
                  <Label resid="BtnLimpiar.Label"/>
                  <Supertip>
                    <Title resid="BtnLimpiar.Label"/>
                    <Description resid="BtnLimpiar.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>limpiarFormato</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="BtnFecha">
                  <Label resid="BtnFecha.Label"/>
                  <Supertip>
                    <Title resid="BtnFecha.Label"/>
                    <Description resid="BtnFecha.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>insertarFecha</FunctionName>
                  </Action>
                </Control>

              </Group>

            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://basmon123.github.io/Web-Word/EditorFDA/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://basmon123.github.io/Web-Word/EditorFDA/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://basmon123.github.io/Web-Word/EditorFDA/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://basmon123.github.io/Web-Word"/>
        <bt:Url id="Commands.Url" DefaultValue="https://basmon123.github.io/Web-Word/EditorFDA/src/taskpane/taskpane.html"/>
        <bt:Url id="Taskpane.Url" DefaultValue="https://basmon123.github.io/Web-Word/EditorFDA/src/taskpane/taskpane.html"/>
      </bt:Urls>
      
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="¡Bienvenido al Editor FDA!"/>
        <bt:String id="TabFDA.Label" DefaultValue="FDA Herramientas"/>
        <bt:String id="GrupoFDA.Label" DefaultValue="Gestión Documental"/>
        <bt:String id="GrupoAcciones.Label" DefaultValue="Acciones Rápidas"/>
        
        <bt:String id="TaskpaneButton.Label" DefaultValue="Generador Documentos"/>
        <bt:String id="BtnLimpiar.Label" DefaultValue="Formato FDA"/>
        <bt:String id="BtnFecha.Label" DefaultValue="Insertar Fecha"/>
      </bt:ShortStrings>
      
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Complemento FDA cargado."/>
        <bt:String id="TaskpaneButton.Tooltip" DefaultValue="Abre el panel para generar documentos."/>
        <bt:String id="BtnLimpiar.Tooltip" DefaultValue="Convierte el texto seleccionado a Arial 11, Justificado."/>
        <bt:String id="BtnFecha.Tooltip" DefaultValue="Inserta la fecha actual en el cursor."/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>